<%= simple_form_for(car) do |f| %>
  <%= f.input :model %>
  <%= f.input :price %>
  <%= f.input :seat_number %>
  <%= f.input :photos, as: :file, input_html: { multiple: true } %>
  <%= f.input :address %>
  <%= f.input :description %>
  <%= f.input :opening_date,
            as: :string,
            input_html: { data: { controller: "datepicker" } } %>
  <%= f.submit class:"btn btn-success"%>
<% end %>
